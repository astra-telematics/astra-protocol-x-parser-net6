<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>AstraProtocolXParser</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>AstraProtocolXParser</PackageId>
    <Version>1.0.0</Version>
    <Authors>Astra Telematics Limited</Authors>
    <Company>Astra Telematics Limited</Company>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/astra-telematics/astra-protocol-x-parser-net6</PackageProjectUrl>
    <RepositoryUrl>https://github.com/astra-telematics/astra-protocol-x-parser-net6</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
        <None Include="../README.md" Pack="true" PackagePath="\"/>
        <None Include="../LICENSE" Pack="true" PackagePath="\"/>
    </ItemGroup>

</Project>
